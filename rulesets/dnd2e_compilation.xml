  <windowclass name="cta_skills_host" merge="join">
    <script>
      function onInit()
      if child and child[2] then
        if child[2].backcolor and child[2].backcolor[1] then
        child[2].backcolor[1] = "[232323]"
        end
      end
      end
    </script>
  </windowclass>

